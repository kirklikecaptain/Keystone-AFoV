<div class="artists-bg">
	<div class="limiter artists-grid">
		{{#each artists}}
			<a class="artist-card" href="{{artistUrl slug}}">
				<div class="artist-card-pic-box">
					<img class="artist-card-pic shadow" src="{{artistPicture.secure_url}}" style="border: 10px solid white; "alt="">
				</div>
				<h2 class="artist-card-name shadow" style="background: {{artistColor}};">{{artistName}}</h2>
			</a>
		{{/each}}
	</div>
</div>
